task.spawn(function()
    task.wait(20)
    local chatrem = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest
    while task.wait(1) do

        chatrem:FireServer('mua gems gia sieu re tai nocblox . c o m', "All")
        wait(2)
        chatrem:FireServer('gems toilet va anime gia re tai nocblox . c o m', "All")
        wait(2)
        chatrem:FireServer('mua unit chu ky gia sieu re tai nocblox . c o m', "All")
        wait(2)
        chatrem:FireServer('mua ạc blocfruits gia sieu re tai n ᴏ c b ʟ ᴏ x . c ᴏ m', "All")
        wait(2)
        chatrem:FireServer('mua gems gia sieu re taii nocblox . c o m', "All")
        wait(2)
        chatrem:FireServer('gems toilet va anime gia re tai nocblox . c o m', "All")
        wait(2)
        chatrem:FireServer('ae mua gems toilet va anime tai nocblox . c o m', "All")
        wait(2)
        chatrem:FireServer('ae mua ẹc bloc phút giá gẻ tai nocblox . c o m', "All")
        wait(2)
        chatrem:FireServer('mua gems gia sieu re tai nocblox . c o m', "All")
        wait(2)
        chatrem:FireServer('mua unit chu ky gia sieu re tai nocblox . c o m', "All")
        wait(1)
    end
end)
